{
  "template": "cyt.*",
  "version": 9,
  "settings": {
    "index.refresh_interval": "1s",
    "index.mapping.total_fields.limit": 300000,
    "index.mapping.depth.limit": 20000,
    "index.number_of_shards": 1,
    "index.number_of_replicas": 0,
    "index.merge.scheduler.max_thread_count": 4,
    "index.routing.allocation.include.storage": "hot",
    "analysis": {
      "normalizer": {
        "c_normalizer": {
          "filter": [
            "lowercase",
            "asciifolding"
          ],
          "type": "custom"
        }
      },
      "analyzer": {
        "c_analyzer": {
          "filter": [
            "lowercase",
            "asciifolding",
            "word_delimiter"
          ],
          "type": "custom",
          "tokenizer": "standard"
        }
      }
    },    
  "mappings": {
    "doc": {
      "dynamic": "true",
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "@version": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          }
        },
        "beat": {
          "properties": {
            "hostname": {
              "type": "text",
              "fields": {
                "keyword": {
                  "type": "keyword",
                  "ignore_above": 256
                }
              }
            },
            "timezone": {
